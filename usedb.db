use smokymountaincup2024; 

CREATE TABLE Equipos (
    id INT AUTO_INCREMENT PRIMARY KEY,
    nombre VARCHAR(50) NOT NULL,
    estado VARCHAR(50) NOT NULL,
    categoria ENUM('Premier', 'Primera', 'Femenil') NOT NULL
);

